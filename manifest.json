{
  "manifest_version": 2,
  "name": "Twitter Hide Discover More",
  "version": "1.0.0",
  "description": "Hide some elements with Discover more",
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*/status/*","*://mobile.twitter.com/*/status/*"],
      "js": ["hide.js"],
						"run_at": "document_end"
    }
  ]
}

